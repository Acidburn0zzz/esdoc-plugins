{
  "source": "./test/fixture/src",
  "destination": "./test/fixture/out",
  "index": "./test/fixture/README.md",
  "package": "./test/fixture/package.json",
  "title": "ESDoc Test Fixture",
  "plugins": [
    {
      "name": "./src/index.js",
      "option": {
        "manual": {
          "files": ["./test/fixture/manual/overview.md"]
        },
        "test": {
          "source": "./test/fixture/test",
          "includes": ["Test.js$"]
        }
      }
    },
    {
      "name": "./test/fixture/spy-plugin.js"
    }
  ]
}
